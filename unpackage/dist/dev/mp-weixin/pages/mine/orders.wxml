<view class="orders-container"><view class="orders-header"><view class="title">我的预约</view></view><view wx:if="{{a}}" class="order-list"><view wx:for="{{b}}" wx:for-item="item" wx:key="n" class="order-item"><view class="order-top"><view class="order-id">订单号：{{item.a}}</view><view class="{{['order-status', item.c]}}">{{item.b}}</view></view><view class="order-info"><view class="service-name"><text class="label">服务项目：</text><text class="value">{{item.d}}</text></view><view class="appointment-time"><text class="label">预约时间：</text><text class="value">{{item.e}} {{item.f}}</text></view><view class="duration"><text class="label">预计时长：</text><text class="value">{{item.g}}分钟</text></view><view wx:if="{{item.h}}" class="remark"><text class="label">备注信息：</text><text class="value">{{item.i}}</text></view></view><view class="order-bottom"><text class="create-time">{{item.j}}</text><view class="order-btns"><view wx:if="{{item.k}}" class="btn cancel-btn" bindtap="{{item.l}}">取消预约</view><view class="btn detail-btn" bindtap="{{item.m}}">查看详情</view></view></view></view></view><view wx:else class="empty-state"><image src="{{c}}" mode="aspectFit" class="empty-img"></image><view class="empty-text">暂无预约记录</view><view class="go-btn" bindtap="{{d}}">去预约</view></view></view>